version: 2

models:
  - name: stg_prices  # Must match filename 'stg_prices.sql'
    description: "Cleaned price data from CoinGecko"
    columns:
      - name: symbol
        description: "The coin ticker (e.g., bitcoin)"
        data_tests:
          - unique
          - not_null
      - name: usd_price
        data_tests:
          - not_null